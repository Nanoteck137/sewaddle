{
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSameLine": false,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "importOrderTypeScriptVersion": "5.0.0"
}